{
  "name": "simple-todo",
  "version": "0.0.1",
  "license": "private",
  "private": true,
  "scripts": {
    "build:dev": "rimraf ./lib/**/* && tsc",
    "build": "rimraf ./lib/**/* && tsc -p tsconfig-build.json --watch",
    "build:prod": "rimraf ./lib/**/* && tsc -p tsconfig-build.json && rimraf ./tmp",
    "serve:dev": "NODE_ENV=development concurrently \"sleep 7s && npm run migrate:up\" \"sleep 3s && npm run start:code\"",
    "serve:dev:publish": "NODE_ENV=development concurrently \"npm run build:dev\" \"ngrok http 5001\" \"npm run start:code\"",
    "start:code": "NODE_ENV=development nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"node --trace-warnings --inspect -r ts-node/register src/main.ts\"",
    "serve": "npm run build:dev && node ./lib/main",
    "start": "npm run build && npm run shell",
    "lint": "tslint -p tsconfig.json src/**/*.ts -e tests/**/*.*",
    "test": "NODE_ENV=development jest --detectOpenHandles --forceExit",
    "test:debug": "NODE_ENV=development node --inspect-brk ./node_modules/jest/bin/jest.js --forceExit --runInBand",
    "migrate:up": "node -r ts-node/register ./src/utils/seeder-runner.ts up",
    "migrate:down": "node -r ts-node/register ./src/utils/seeder-runner.ts down"
  },
  "dependencies": {
